<div class="px-4 py-5 my-5 text-center">
    @AboveTitleContent
    <h1 class="display-5 fw-bold text-body-emphasis">@LargeTitle</h1>
    <div class="col-lg-6 mx-auto">
        @Body
    </div>
</div>
@code {
    [Parameter]
    public string? LargeTitle { get; set; }
    [Parameter]
    public RenderFragment? Body { get; set; }
    [Parameter]
    public RenderFragment? AboveTitleContent { get; set; }

}
